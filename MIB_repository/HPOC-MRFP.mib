SdspceConnectionState ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Enum: Sdspce (HP Soft DSP conference Engine for video) different states
	       - not-responding  : The Soft DSP conference Engine process is not answering
               - up              : The Soft DSP conference Engine process is up and running
               - down            : The Soft DSP conference Engine process has shut down
               - invalid-license : The Soft DSP conference Engine's license file not present or invalid
	       - stopping        : The Soft DSP conference Engine is stopping: a Gracefull shutdown (maintenance) has been triggered"
	SYNTAX		INTEGER { not-responding(0), up(1), down(2), invalid-license(3), stopping(4) }

SdspceOverloadState ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Enum: Sdspce overload different states
               - normal : normal Sdspce overload state
               - high   : current Sdspce load is close to the limit
               - hit    : current Sdspce load has reached the limit"
	SYNTAX		INTEGER { normal(7), high(8), hit(9) }


LicensingState ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Enum: Mrfp Licensing different states
               - normal : normal licensing state
               - high   : current the number of RTP ports used are close to the license limit
               - hit    : current the number of RTP ports used have reached the license limit"
	SYNTAX		INTEGER { normal(0), high(1), hit(2) }


OverloadState ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Enum: Mrfp overload different states
               - normal : normal Mrfp overload state
               - high   : current Mrfp load is close to the limit
               - hit    : current Mrfp load has reached the limit"
	SYNTAX		INTEGER { normal(0), high(1), hit(2) }


MrfcConnectionState ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION "Enum: Mrfp - Mrfc connection different states
	           - unknown        : The Mrfp-Mrfc initial connection state
               - off            : The Mrfp-Mrfc connection has been tore down (Mrfp shutdown)
               - connecting     : The Mrfp is trying to connect to one of the configured Mrfc
               - stopping       : The Mrfp is shutting down
	           - handing-off    : The last active Mrfc has requested the Mrfp a hand-off to a another Mrfc
	           - failed         : The Mrfp-Mrfc connection has an invalid conguration
               - re-connecting  : The Mrfp has been disconnected and is trying to re-connect to a configured Mrfc"
	SYNTAX		INTEGER { unknown(0), off(1), connecting(2), connected(3), stopping(4), handing-off(5), failed(6), re-connecting(7) }


MrfcState ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Enum: Mrfc different states
	           - unknown     : The Mrfc has not exchanged any ServiceChange message yet
               - handoff     : The Mrfc has retired (hand off)
               - stopping    : The Mrfc is stopping: a Gracefull or a Forced shutdown has been triggered but the Mrfc timeout has not expired yet
               - failed      : The Mrfc has been found failed, he will be the last re-used in case of a search for a new Mrfc to connect to
	           - active      : The Mrfc is currently active
	           - handing-off : The Mrfc has required a handoff"
	SYNTAX		INTEGER { unknown(0), handoff(1), stopping(2), failed(3), active(4), handing-off(5) }


