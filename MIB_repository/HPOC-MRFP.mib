ConfigurationContentState ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Enum: Configuration file content different states
		       - invalid : Current Mrfp configuration file content is invalid
		       - updated : Current Mrfp configuration file content has been updated"
	SYNTAX		INTEGER { invalid(0), updated(1) }


ConfigurationAccessState ::= TEXTUAL-CONVENTION
	STATUS		current
	DESCRIPTION
		"Enum: Configuration server different states
		        - primary     : Primary server is used for Mrfp configuration
		        - backup      : Backup server is used for Mrfp configuration (primary server unavailable)
		        - local       : Local server is used for Mrfp configuration
		        - unavailable : No configuration server are available for Mrfp configuration"
	SYNTAX		INTEGER { primary(0), backup(1), local(2), unavailable(3) }



