-- =====================================================================================================================================
-- trap definition (coming from a OC products standard trap events ) : mrfp.1
-- =====================================================================================================================================

-- Active Alarm Trap table

trapTable    OBJECT-TYPE
SYNTAX  SEQUENCE OF     TrapEntry
MAX-ACCESS      not-accessible
STATUS  current
DESCRIPTION     "A list of the Mrfp current active (not cleared) alarm/traps and their last emission date"
::=     {       status      6
        }

trapEntry    OBJECT-TYPE
SYNTAX  TrapEntry
MAX-ACCESS      not-accessible
STATUS  current
DESCRIPTION     "The entry for the active alarm trapTable"
INDEX {  trapName  }
::=     {       trapTable    1
        }

TrapEntry    ::=  SEQUENCE   {
                                    trapName DisplayString,
                                    trapOid DisplayString,
                                    trapPerceivedSeverity HpocPerceivedSeverity,
                                    trapEventTime DisplayString,
                                    trapTrendIndication INTEGER,
                                    trapNotificationIdentifier DisplayString,
                                    trapEventType HpocEventType,
                                    trapOtherEventType DisplayString,
                                    trapAdditionalInformation DisplayString,
                                    trapProbableCause HpocProbableCause,
                                    trapProposedRepairActions DisplayString,
                                    trapCorrelatedNotifications DisplayString,
                                    trapAdditionalText DisplayString,
                                    trapManagedObject DisplayString,
                                    trapI18NAlarmText DisplayString,
                                    trapOldState INTEGER,
                                    trapNewState INTEGER
                             }


trapName     OBJECT-TYPE
SYNTAX  DisplayString
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap name"
::=     {       trapEntry    1
        }


trapOid     OBJECT-TYPE
SYNTAX  DisplayString
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap OID"
::=     {       trapEntry    2
        }

trapPerceivedSeverity     OBJECT-TYPE
SYNTAX  HpocPerceivedSeverity
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap associated severity"
::=     {       trapEntry    3
        }

trapEventTime     OBJECT-TYPE
SYNTAX  DisplayString
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap associated timestamp"
::=     {       trapEntry    4
        }

trapTrendIndication     OBJECT-TYPE
SYNTAX  INTEGER {
			unknown(0),
			notApplicable(1),
			trendingUp(2),
			trendingDown(3),
			noChange(4)
			    }
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap associated trend"
::=     {       trapEntry    5
        }

trapNotificationIdentifier     OBJECT-TYPE
SYNTAX  DisplayString
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap associated notification identifier"
::=     {       trapEntry    6
        }


trapEventType     OBJECT-TYPE
SYNTAX  HpocEventType
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap associated category"
::=     {       trapEntry    7
        }

trapOtherEventType     OBJECT-TYPE
SYNTAX  DisplayString
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap associated other category"
::=     {       trapEntry    8
        }


trapAdditionalInformation     OBJECT-TYPE
SYNTAX  DisplayString
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap associated additional information"
::=     {       trapEntry    9
        }


trapProbableCause     OBJECT-TYPE
SYNTAX  HpocProbableCause
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap associated probable cause"
::=     {       trapEntry    10
        }

trapProposedRepairActions     OBJECT-TYPE
SYNTAX  DisplayString
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap associated possible actions to apply"
::=     {       trapEntry    11
        }

trapCorrelatedNotifications     OBJECT-TYPE
SYNTAX  DisplayString
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap correlated alarms"
::=     {       trapEntry    12
        }

trapAdditionalText     OBJECT-TYPE
SYNTAX  DisplayString
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap description text"
::=     {       trapEntry    13
        }


trapManagedObject     OBJECT-TYPE
SYNTAX  DisplayString
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap originator OID"
::=     {       trapEntry    14
        }

trapI18NAlarmText     OBJECT-TYPE
SYNTAX  DisplayString
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap associated alarm text"
::=     {       trapEntry    15
        }

trapOldState     OBJECT-TYPE
SYNTAX  INTEGER
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap associated old state"
::=     {       trapEntry    16
        }

trapNewState     OBJECT-TYPE
SYNTAX  INTEGER
MAX-ACCESS      read-only
STATUS  current
DESCRIPTION      "Trap associated new state"
::=     {       trapEntry    17
        }

